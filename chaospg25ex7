# File: chaospg25ex7.py
# A simple to program illustrating chaotic bahavior

def main():
    print("This program illustrates a chaotic function")
    x = eval(input("Enter a number between 0 and 1: "))
    y = eval (input("Next, enter a different number between 0 and 1: "))
    for i in range(10):
        x = 3.9 * x - 3.9 * x * x
        y = 3.9 * y - 3.9 * y * y
        print(x,y)

main()
